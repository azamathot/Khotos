{
  "Keycloak": {
    "realm": "Khotos",
    "auth-server-url": "http://Keycloak:8080/",
    "ssl-required": "none",
    "resource": "khotos-client",
    "public-client": true,
    "verify-token-audience": true,
    "use-resource-role-mappings": true,
    "confidential-port": 0
  },
  "RabbitMQ": {
    "HostName": "rabbitmq",
    "Port": 5672,
    "UserName": "guest",
    "Password": "guest",
    "QueueName": "my_queue" //  Добавляем  имя  очереди    
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=sql_server2022;Database=Khotos_Products;User Id=SA;Password=[password];MultipleActiveResultSets=true;Trust Server Certificate=True;Encrypt=False"
  }
}
