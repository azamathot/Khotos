{
  "WebGatewayUrl": "https://localhost:5050",
  "Keycloak": {
    "realm": "Khotos",
    "auth-server-url": "http://localhost:8080/",
    "ssl-required": "none",
    "resource": "khotos-client",
    "public-client": true,
    "verify-token-audience": true,
    "use-resource-role-mappings": true,
    "confidential-port": 0
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "HostNameDocker": "rabbitmq",
    "Port": 5672,
    "UserName": "guest",
    "Password": "guest",
    "QueueName": "my_queue" //  Добавляем  имя  очереди    
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
