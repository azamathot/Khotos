{
  "Keycloak": {
    "realm": "Khotos",
    "auth-server-url": "http://localhost:8080/",
    "ssl-required": "none",
    "resource": "khotos-client",
    "public-client": true,
    "verify-token-audience": true,
    "use-resource-role-mappings": true,
    "confidential-port": 0
  },
  "ConnectionStrings": {
    "DefaultConnection1": "Server=(localdb)\\mssqllocaldb;Database=Khotos_Orders;MultipleActiveResultSets=true;Trust Server Certificate=True;Encrypt=False",
    "DefaultConnection": "Server=localhost, 1433;Database=Khotos_Orders;User Id=SA;Password=[password];MultipleActiveResultSets=true;Trust Server Certificate=True;Encrypt=False"
  }
}
